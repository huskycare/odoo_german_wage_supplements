<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="rcs_view_resource_calendar_leaves_tree" model="ir.ui.view">
            <field name="name">resource.calendar.leaves.tree</field>
            <field name="model">resource.calendar.leaves</field>
            <field name="inherit_id" ref="resource.resource_calendar_leave_tree"/>
            <field name="arch" type="xml">
                <xpath expr="field[@name='name']" position="after">
                    <field name="holiday_high"/>
                </xpath>
            </field>
         </record>
    </data>
</odoo>